{
  "name": "@project/components",
  "version": "1.0.0",
  "main": "src/index.js",
  "main:src": "src/index.js",
  "scripts": {
    "test": "CI=true react-scripts test",
    "lint": "eslint ./src/**/*.js --max-warnings=0 --format=codeframe"
  },
  "dependencies": {
    "@ambroseus/react-scripts": "3.2.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/serviceWorker.{js,jsx,ts}",
      "!src/**/*.(spec|test|stories).{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 98,
        "branches": 98,
        "functions": 98,
        "lines": 98
      }
    }
  },
  "license": "MIT"
}
